package assignment.kpmg.com;

import java.util.Scanner;

public class question5 {
	public static void main(String[] args) {
	int a,b,c,d,e;
    Scanner sc =new Scanner(System.in);
    System.out.println("Enter the 5 number:");
    a=sc.nextInt();
    b=sc.nextInt();
    c=sc.nextInt();
    d=sc.nextInt();
    e=sc.nextInt();
    
    System.out.println("Sum of the 5 numberis:"+(a+b+c+d+e));

    System.out.println("Average of the 5 numberis:"+(a+b+c+d+e)/5);

}}
